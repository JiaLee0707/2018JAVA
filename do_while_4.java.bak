import java.util.*;

class do_while_4 {
  public static void main(String ar[]) {
    Scanner sc=new Scanner(System.in);

	//이름, 나이를 입력받아 5번 출력
    System.out.print("이름을 입력하시오 : ");
    String name=sc.next();
    System.out.print("나이를 입력하시오 : ");
    int num=sc.nextInt();
    int count=0;
    do {
		System.out.println("이름 :"+name+" 나이 :"+num);
		count++;
    }while(count<=5);
 }
}