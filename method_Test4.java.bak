//4. 두 수 사이의 합구기
//리턴값 x, 매개변수 x

import java.util.*;
class method_Test4 {
	public void a() {
		Scanner sc=new Scanner(System.in);
		int sum=0;
		System.out.print("두 수 사이의 합을 구할 숫자 2개를 입력하시오 : ");
		int x=sc.nextInt();
		int y=sc.nextInt();
		for(int i=x+1; i<y; i++) {
			sum+=i;
		}
		System.out.println(x+"와 "+y+" 두 수 사이의 합 : "+sum);
	}

	public static void main(String ar[]) {
		method_Test4 m=new method();
		m.a();
	}
}