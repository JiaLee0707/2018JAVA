import java.util.*;

class num2 {
  public static void main(String ar[]) {
    Scanner sc=new Scanner(System.in);

    int c=0;
    int n=sc.nextInt();
    for(int i=1; i<=n; i++) {
		for(int j=i; j<=i+3; j++) {
			System.out.print(j);
			c=c+1;
			if(c==4) {
				System.out.println();
				c=0;
			}
		}
	}
  }
}